
<% include ('../layouts/header.ejs') %>

<style>
  /* Your styles for the h1 tag */
  h1 {
    color: #0066cc; /* Set text color to blue */
    font-size: 2em;
    margin-left: 40%; /* Set font size to 2em (adjust as needed) */
    /* Add more styles as needed */
  }

  /* Your styles for the table */
  table {
    width: 100%;
    /* Add more styles as needed */
  }

  /* Your styles for the table cells */
  td {
    padding: 10px;
    border: 1px solid #ccc;
    /* Add more styles as needed */
  }

  

  /* Add more styles for other elements as needed */
